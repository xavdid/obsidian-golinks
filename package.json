{
  "name": "obsidian-golinks",
  "version": "0.1.0",
  "description": "This is a plugin for Obsidian that renders `go/links` clickable.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src",
    "test": "jest"
  },
  "keywords": [
    "obsidian"
  ],
  "author": {
    "name": "David Brownman",
    "email": "beamneocube@gmail.com",
    "url": "https://xavd.id"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "eslint": "^8",
    "jest": "^28.1.2",
    "obsidian": "latest",
    "ts-jest": "^28.0.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest"
  }
}
